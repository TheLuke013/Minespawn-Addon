{
	"format_version": "1.20.80",
	"minecraft:client_entity": {
		"description": {
			"identifier": "twisted:robo_laser",
			"materials": {
				"default": "arrow"
			},
			"textures": {
				"default": "textures/entity/robos/jeffery"
			},
			"geometry": {
				"default": "geometry.robo_laser"
			},
			"render_controllers": [
				"controller.render.robo_jeffery"
			],
			"animations": {
				"move": "animation.rocket.move"
			},
			"scripts": {
				"pre_animation": [
					"variable.shake = query.shake_time - query.frame_alpha;",
					"variable.shake_power = variable.shake > 0.0 ? -Math.sin(variable.shake * 200.0) * variable.shake : 0.0;"
				],
				"animate": [
					"move"
				]
			}
		}
	}
}