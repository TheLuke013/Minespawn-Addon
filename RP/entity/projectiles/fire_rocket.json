{
	"format_version": "1.20.80",
	"minecraft:client_entity": {
		"description": {
			"identifier": "twisted:fire_rocket",
			"materials": {
				"default": "arrow"
			},
			"textures": {
				"default": "textures/blasters/fire_blaster"
			},
			"geometry": {
				"default": "geometry.rocket"
			},
			"render_controllers": [
				"controller.render.item_sprite"
			],
			"animations": {
				"move": "animation.rocket.move"
			},
			"scripts": {
				"pre_animation": [
					"variable.shake = query.shake_time - query.frame_alpha;",
					"variable.shake_power = variable.shake > 0.0 ? -Math.sin(variable.shake * 200.0) * variable.shake : 0.0;"
				],
				"animate": [
					"move"
				]
			}
		}
	}
}