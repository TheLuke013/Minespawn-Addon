{
	"format_version": "1.20.80",
	"minecraft:client_entity": {
		"description": {
			"identifier": "minespawn:nastysaurus",
			"materials": {
				"default": "entity_alphatest"
			},
			"textures": {
				"default": "textures/entity/nastysaurus"
			},
			"geometry": {
				"default": "geometry.nastysaurus"
			},
			"render_controllers": [
				"controller.render.nastysaurus"
			],
			"spawn_egg": {
				"base_color": "#000000",
				"overlay_color": "#FFFFFF"
			},
			"animations": {
				"idle": "animation.nastysaurus.idle",
				"walk": "animation.nastysaurus.walk",
				"melee_attack": "animation.nastysaurus.attack",
				"melee_attack_controller": "controller.animation.warden.melee_attacking",
				"run_attack": "animation.nastysaurus.run_attack",
				"look_at_target": "animation.nastysaurus.look_at_target"
			},
			"scripts": {
				"animate": [
					"look_at_target",
					{
						"idle": "query.modified_move_speed <= 0.01"
					},
					{
						"walk": "query.modified_move_speed >= 0.1 && query.modified_move_speed < 0.5"
					},
					{
						"run_attack": "query.modified_move_speed >= 0.5"
					}
				]
			}
		}
	}
}