{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.robo_jeffery": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"mega_smash": "query.skin_id == 4"
						}
					]
				},
				"mega_smash": {
					"transitions": [
						{
							"default": "query.skin_id != 5"
						}
					],
					"on_entry": [
						"/tag @s add mega_smash_attack",
						"/playsound blaster.mega_shot @p[r=30]"
					]
				}
			}
		}
	}
}