{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.mobjira_destroy": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"destroy": "(1.0)"
						}
					],
					"on_entry": [
						"/function mobjira_destroy"
					]
				},
				"destroy": {
					"transitions": [
						{
							"default": "(1.0)"
						}
					],
					"on_entry": [
						"/function mobjira_destroy"
					]
				}
			}
		},
		"controller.animation.mobjira_lightning": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"lightning": "query.skin_id == 3"
						}
					]
				},
				"lightning": {
					"transitions": [
						{
							"default": "query.skin_id != 3"
						}
					],
					"on_entry": [
						"/execute as @e[rm=0,r=100,family=!mobjira,family=!inanimate,type=!lightning_bolt,type=!item] at @s run summon lightning_bolt"
					]
				}
			}
		},
		"controller.animation.mobjira_jump": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"jump": "query.is_jumping || query.skin_id == 4"
						}
					]
				},
				"jumping": {
					"transitions": [
						{
							"default": "query.skin_id != 6"
						}
					],
					"on_entry": [
						"/playsound mob.mobjira.jump_impact @p[r=100]",
						"/execute as @e[rm=0,r=100,c=1,family=!mobjira] at @s run tag @p[r=100] add mobjira_jump_knockback",
						"/execute as @e[rm=0,r=100,c=1,family=!mobjira] at @s run camerashake add @p[r=100] 4 1 positional"
					]
				},
				"jump": {
					"transitions": [
						{
							"jumping": "query.skin_id != 4 && query.is_on_ground"
						}
					],
					"on_entry": [
						"/effect @s levitation 1 15 true",
						"/tag @s add mobjira_is_jumping"
					]
				}
			}
		},
		"controller.animation.mobjira_atomic": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"atomic": "query.health <= 4000 && query.health != 0 && query.skin_id < 7"
						},
						{
							"atomic_shock": "query.skin_id == 9"
						}
					]
				},
				"atomic": {
					"transitions": [
						{
							"default": "query.skin_id != 7"
						}
					],
					"on_entry": [
						"/playsound mob.mobjira.atomic_breath @p[r=100] ~~~ 100 1 100",
						"/tag @s remove ready_for_breath",
						"/event entity @s twisted:prepare_for_breath_atomic",
						"/execute as @e[rm=0,r=100,c=1,family=!mobjira] at @s run camerashake add @p[r=100] 1 11 positional"
					]
				},
				"atomic_shock": {
					"transitions": [
						{
							"default": "query.skin_id != 9"
						}
					],
					"on_entry": [
						"/particle twisted:atomic_shock ~~~",
						"/function mobjira_ultimate_destroy",
						"/summon twisted:mj_atomic_explosion ~~~"
					]
				}
			}
		}
	}
}